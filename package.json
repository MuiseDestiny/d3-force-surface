{
  "name": "d3-force-surface",
  "version": "0.5.1",
  "description": "A multi-surface elastic collision force type for the d3-force simulation engine.",
  "keywords": [
    "d3",
    "d3-module",
    "d3-force",
    "surface",
    "bounce",
    "impact",
    "collision",
    "physics",
    "layout",
    "graph",
    "force",
    "simulation"
  ],
  "homepage": "https://github.com/vasturiano/d3-force-surface",
  "license": "MIT",
  "author": {
    "name": "Vasco Asturiano",
    "url": "https://bl.ocks.org/vasturiano"
  },
  "main": "dist/d3-force-surface.js",
  "module": "src/index",
  "jsnext:main": "src/index",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasturiano/d3-force-surface.git"
  },
  "scripts": {
    "build": "rm -rf dist && mkdir dist && rollup -c --banner \"$(preamble)\"",
    "watch": "rollup -c -w",
    "minify": "uglifyjs dist/d3-force-surface.js -o dist/d3-force-surface.min.js -c -m --preamble \"$(preamble)\"",
    "prepublish": "npm run build && npm run minify"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "package-preamble": "~0.0.2",
    "rollup": "~0.41.6",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-node-resolve": "^3.0",
    "rollup-watch": "^3.2",
    "uglify-js": "^3.0.28"
  }
}
